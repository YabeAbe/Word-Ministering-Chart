<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wordministeringchart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WordMinisteringChart">
        <activity android:name=".AddFamilyMember"></activity>
        <activity android:name=".AddNewFamily" />
        <activity android:name=".DisplayFamilyDetail" />
        <activity android:name=".DisplayFamiliesList" />
        <activity android:name=".DeletePerson" />
        <activity android:name=".EditPerson" />
        <activity android:name=".DisplayPerson" />
        <activity android:name=".AddNewPerson" />
        <activity android:name=".DisplayPeopleList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>